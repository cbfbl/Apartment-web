      th Dish
      th Refresher
      th Garbage
    tr
      td
        canvas#can1
      td
        canvas#can2
      td
        canvas#can3
      td
        canvas#can4 
    tr
      td
        button#prev1(type='submit') prev
        button#next1(type='submit') next
